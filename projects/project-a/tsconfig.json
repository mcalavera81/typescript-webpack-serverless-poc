{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "outDir": "./.build",
    "rootDir": "../../",
    "allowJs": true,
    "sourceMap": true,
    "typeRoots": ["node_modules/@types"],
    "types": ["node"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "strict": true,
    "noImplicitReturns": true,
    //"noUnusedLocals": true,
    //"noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./**/*.ts"],
  "exclude": [
    ".git/**/*",
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ]
}
